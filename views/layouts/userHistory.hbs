<div class="col-md-12">
  <h2>Log In</h2>
  <hr />
  <div class="table-responsive">
    <table class="table">
      <thead>
        <tr>
          <th>Login Date/Time</th>
          <th>Client Information</th>
        </tr>
      </thead>
      <tbody>
        {{#if data.history}}
          {{#each data.history}}
            <tr>
              <td>{{dateTime}}</td>
              <td>{{userAgent}}</td>
            </tr>
          {{/each}}
        {{/if}}
      </tbody>
    </table>
  </div>
</div>
